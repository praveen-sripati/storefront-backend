{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "nodemon src/server.ts",
    "build": "npm-run-all lint prettier tsc",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
    "lint": "eslint '*/**/*.ts' --quiet --fix",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "create-db": "db-migrate db:create storefront",
    "delete-db": "db-migrate db:create storefront",
    "create-test-db": "db-migrate db:create storefront_test",
    "delete-test-db": "db-migrate db:drop storefront_test",
    "test": "npm run create-test-db && db-migrate --env test up && ENV=test jasmine-ts && npm run delete-test-db",
    "tsc": "npx tsc"
  },
  "author": "Praveen Sripati",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "db-migrate": "^1.0.0-beta.16",
    "db-migrate-pg": "^1.2.2",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "nodemon": "^2.0.13",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.1.3"
  }
}
